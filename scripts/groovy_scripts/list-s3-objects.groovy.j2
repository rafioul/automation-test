def region = "{{ aws_region }}"
def bucketname = "{{ artifact_s3_bucket }}"
def folder = "{{ folder_name }}"
def prefix = "{{ artifact_prefix }}-"

def result = []
def command = "aws s3 ls --region ${region} s3://${bucketname}/${folder}/${prefix}"
command.execute().text.eachLine { result.add(it.tokenize(" ")[-1]) }
return result
